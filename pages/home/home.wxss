/*遮罩层*/
.list-mask {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 40;
    backdrop-filter: blur(10rpx);
    opacity: 1;
    background: rgba(7, 17, 27, 0.6);
}

.list-mask.fade-enter-active, .list-mask.fade-leave-active {
    transition: all 0.5s;
}

.list-mask.fade-enter, .list-mask.fade-leave-to {
    opacity: 0;
    background: rgba(7, 17, 27, 0);
}

.list-mask .logo {
    position: absolute;
    right: 20rpx;
    top: 20rpx;
    color: white;
    font-size: 30rpx;
}
